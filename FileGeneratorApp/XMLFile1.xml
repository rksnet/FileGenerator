<FileGenerator>
  <CommonDetails>
    <Separater>:</Separater>
    <FileName>XYZ{mmDDyyyy}</FileName>
    <GenrateHeader>  True</GenrateHeader>
  </CommonDetails>

  <HeaderDetails>
    <ID/>
    <FirstName/>
    <Lastname/>
    <Amount/>
  </HeaderDetails>

  <Columns>
    <ID  Datatype ="int" Identity ="true"       NegativeValue="0" />
    <FirstName  Datatype ="String" Length ="10" NegativeValue="20" />
    <Lastname   Datatype ="String" Length ="20" NegativeValue="0" />
    <Amount     Datatype ="int"                 NegativeValue="0" />
    <Course     Datatype ="string"              NegativeValue="0" />
  </Columns>

  <FooterDetails>
    <RecordCount Value="100"/>
  </FooterDetails>

</FileGenerator>